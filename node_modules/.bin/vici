#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/api/bin/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/api/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/api/bin/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/api/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules/@vicinae/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/@vicinae+api@0.15.7_@types+node@24.9.2_@types+react@19.0.10_typescript@5.9.3/node_modules:/home/fryad/projects/other/vicinae/zen-bookmarks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vicinae/api/bin/run.js" "$@"
else
  exec node  "$basedir/../@vicinae/api/bin/run.js" "$@"
fi
